<dataConfig>
	<datasetName>杭州房地产数据</datasetName>

	<attr>
		<fileName>hz_data/final_v0.9.3.csv</fileName>
		<fileType>csv</fileType>
		<key>smid</key>
		<name>楼盘</name>
		<attribute>
			<name>smid</name>
			<colIdx>1</colIdx>
			<dataType>id</dataType>
		</attribute>
		<attribute>
			<name>楼盘</name>
			<colIdx>2</colIdx>
			<dataType>string</dataType>
		</attribute>
		<attribute>
			<name>城区</name>
			<colIdx>3</colIdx>
			<dataType>string</dataType>
		</attribute>
		<attribute>
			<name>物业类型</name>
			<colIdx>4</colIdx>
			<dataType>string</dataType>
		</attribute>
		<attribute>
			<name>开发商</name>
			<colIdx>5</colIdx>
			<dataType>string</dataType>
		</attribute>
		<attribute>
			<name>总套数</name>
			<colIdx>6</colIdx>
			<dataType>int</dataType>
			<summaryType>sum</summaryType>
			<colorMap preset="OR_RD" />
		</attribute>
		<attribute>
			<name>总面积</name>
			<colIdx>7</colIdx>
			<dataType>double</dataType>
		</attribute>
		<attribute>
			<name>住宅套数</name>
			<colIdx>8</colIdx>
			<dataType>int</dataType>
		</attribute>
		<attribute>
			<name>住宅面积</name>
			<colIdx>9</colIdx>
			<dataType>double</dataType>
		</attribute>
		<attribute>
			<name>毛坯均价</name>
			<colIdx>18</colIdx>
			<dataType>double</dataType>
			<summaryType>mean</summaryType>
			<colorMap preset="OR_RD" />
		</attribute>
		<attribute>
			<name>开盘次数</name>
			<colIdx>20</colIdx>
			<dataType>int</dataType>
			<summaryType>sum</summaryType>
			<colorMap preset="OR_RD" />
		</attribute>
		<hierSet>
			<hier>
				<name>城区</name>
				<hier>
					<name>楼盘</name>
				</hier>
			</hier>
		</hierSet>
	</attr>

	<geo>
		<feature>
			<refAttr>楼盘</refAttr>
			<geoType>point</geoType>
			<fileName>map/smid_geo.csv</fileName>
			<fileType>csv</fileType>
			<col>
				<key>2</key>
				<x>4</x>
				<y>5</y>
			</col>
		</feature>
		<feature>
			<refAttr>城区</refAttr>
			<geoType>polygon</geoType>
			<fileName>map/hz_region_8.json</fileName>
			<fileType>geojson</fileType>
		</feature>
		<feature>
			<refAttr>规划单元</refAttr>
			<geoType>polygon</geoType>
			<fileName>map/hz_unit_128.json</fileName>
			<fileType>geojson</fileType>
		</feature>		
		<feature>
			<geoType>layers</geoType>
			<fileName>map/layers.json</fileName>
			<subDir>map/layers</subDir>
			<fileType>geojson</fileType>
		</feature>
	</geo>
	<time>
		<series>
			<fileName>hz_data/sale_data.csv</fileName>
			<name>均价</name>
			<dateCol>0</dateCol>
			<groupCol>3</groupCol>
			<valueCol>6</valueCol>
			<summaryType>mean</summaryType>
		</series>
		<series>
			<fileName>hz_data/sale_data.csv</fileName>
			<name>交易数</name>
			<dateCol>0</dateCol>
			<groupCol>3</groupCol>
			<valueCol>5</valueCol>
			<summaryType>sum</summaryType>
		</series>
		<series>
			<fileName>hz_data/sale_data.csv</fileName>
			<name>预定数</name>
			<dateCol>0</dateCol>
			<groupCol>3</groupCol>
			<valueCol>4</valueCol>
			<summaryType>sum</summaryType>
		</series>
	</time>
</dataConfig>